<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape >
            <corners android:radius="7dp" />
            <gradient
                android:startColor="@color/common_eeeeee"
                android:centerColor="@color/common_eeeeee"
                android:centerY="0.75"
                android:endColor="@color/common_eeeeee"
                android:angle="270" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="7dp" />
                <gradient
                    android:startColor="#00B844"
                    android:endColor="#4CB69F"
                    android:angle="270" />
            </shape>
        </clip>
    </item>
</layer-list>